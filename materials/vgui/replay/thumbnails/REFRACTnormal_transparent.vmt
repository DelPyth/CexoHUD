"Refract"
{
	"%keywords" "tf"
	"$refractamount" "0.0"
	"$refracttint" "{255 255 255}"
	"$refractblur" "0.0"

	"$scale" "[1 1]"

	// With this, the texture won't show if you're on DX 80-ish.
	// Hopefully........
	<dx90
	{
		"$normalmap" "vgui\replay\thumnails\null"
	}

	>=dx90
	{
		"$normalmap" "vgui\replay\thumbnails\REFRACTnormal_transparent"
	}
}
